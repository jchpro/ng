{
  "name": "ng",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project example-app",
    "build": "npm run build:common:prod && npm run build:pico:prod && npm run build:app:prod",
    "build:app:dev": "ng build --project example-app --configuration development",
    "build:app:prod": "ng build --project example-app --configuration production",
    "build:common:dev": "ng build --project @jchpro/ngx-common --configuration development",
    "watch:common:dev": "ng build --watch --project @jchpro/ngx-common --configuration development",
    "build:common:prod": "ng build --project @jchpro/ngx-common --configuration production",
    "build:pico:dev": "ng build --project @jchpro/ngx-pico --configuration development",
    "watch:pico:dev": "ng build --watch --project @jchpro/ngx-pico --configuration development",
    "build:pico:prod": "ng build --project @jchpro/ngx-pico --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test @jchpro/ngx-common --watch=false --browsers=ChromeHeadless",
    "test-local:common": "ng test @jchpro/ngx-common",
    "test-local:pico": "ng test @jchpro/ngx-pico"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.0.5",
    "@angular/common": "^20.0.6",
    "@angular/compiler": "^20.0.6",
    "@angular/core": "^20.0.6",
    "@angular/forms": "^20.0.6",
    "@angular/platform-browser": "^20.0.6",
    "@angular/platform-browser-dynamic": "^20.0.6",
    "@angular/router": "^20.0.6",
    "@fortawesome/angular-fontawesome": "^2.0.1",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@picocss/pico": "^2.1.1",
    "commander": "^13.1.0",
    "dotenv": "^16.6.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.0.5",
    "@angular/cli": "^20.0.5",
    "@angular/compiler-cli": "^20.0.6",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.0.1",
    "typescript": "~5.8.3"
  }
}
