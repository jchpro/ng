{
  "name": "ng",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project example-app",
    "build": "npm run build:common:prod && npm run build:app:prod",
    "build:app:dev": "ng build --project example-app --configuration development",
    "build:app:prod": "ng build --project example-app --configuration production",
    "build:common:dev": "ng build --project @jchpro/ngx-common --configuration development",
    "watch:common:dev": "ng build --watch --project @jchpro/ngx-common --configuration development",
    "build:common:prod": "ng build --project @jchpro/ngx-common --configuration production",
    "build:pico:dev": "ng build --project @jchpro/ngx-pico --configuration development",
    "watch:pico:dev": "ng build --watch --project @jchpro/ngx-pico --configuration development",
    "build:pico:prod": "ng build --project @jchpro/ngx-pico --configuration production",
    "docs:common": "compodoc -p projects/common/tsconfig.lib.doc.json -d docs/common/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test @jchpro/ngx-common --watch=false --browsers=ChromeHeadless",
    "test-local": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.9",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@picocss/pico": "^2.1.1",
    "commander": "^13.1.0",
    "dotenv": "^16.5.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.7",
    "@angular/cli": "^19.2.7",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "compodoc": "^0.0.41",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.0",
    "typescript": "~5.7.2"
  }
}
